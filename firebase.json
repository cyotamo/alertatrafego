{
  "hosting": {
    "public": ".",
    "headers": [
      {
        "source": "/manifest.json",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/manifest+json"
          }
        ]
      }
    ],
    "rewrites": [
      { "source": "/manifest.json", "destination": "/manifest.json" },
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
